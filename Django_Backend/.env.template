# .env.template - copy to .env and fill in values
# Blockchain contract address (after running truffle migrate)
CONTRACT_ADDRESS=0x<deployed_contract_address>
# Local blockchain RPC (Ganache default)
BLOCKCHAIN_URL=http://127.0.0.1:8545
# Django secret key (development only)
SECRET_KEY=change-me-to-a-secret
# Debug mode (True/False)
DEBUG=True
